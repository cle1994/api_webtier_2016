{
  "name": "innod_web_api",
  "version": "0.0.1",
  "description": "API for web tier members to practice with requests",
  "main": "www/server.js",
  "scripts": {
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "build:cp": "cp -r src/static dist/static && mkdir dist/server/templates && cp src/server/templates/index.html dist/server/templates/index.html",
    "build:server": "./node_modules/.bin/babel src -d dist && npm run build:cp",
    "clean": "better-npm-run clean",
    "clean:slate": "better-npm-run clean:slate",
    "cov": "open ./coverage/lcov-report/index.html",
    "dev": "better-npm-run dev",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "nyc:report": "./node_modules/.bin/nyc report",
    "nyc:summary": "./node_modules/.bin/nyc report --reporter=text-summary",
    "setup": "npm install -g better-npm-run eslint babel-cli && npm install && npm run clean",
    "start": "better-npm-run start",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "build": {
      "command": "npm run clean && npm run build:server && node bin/build",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "npm run clean && npm run build:server && node bin/build",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development",
        "NO_HOT": true
      }
    },
    "clean": {
      "command": "node bin/clean.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "clean:slate": {
      "command": "node bin/clean.js --slate",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "./node_modules/.bin/nodemon --delay 500ms --watch src/server www/server",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development",
        "WEBPACK_ENV": "development"
      }
    },
    "start": {
      "command": "node www/server",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "./node_modules/.bin/nyc ./node_modules/.bin/mocha ./test/**/*.spec.js --compilers js:babel-register",
      "env": {
        "NODE_ENV": "test",
        "BABEL_ENV": "test"
      }
    }
  },
  "author": "Christian Le <cle1994>",
  "private": true,
  "dependencies": {
    "airbnb-js-shims": "^1.0.1",
    "axios": "^0.13.1",
    "bluebird": "^3.4.1",
    "boom": "^4.1.0",
    "classnames": "^2.2.5",
    "fs-promise": "^0.5.0",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "h2o2": "^5.1.0",
    "hapi": "^14.1.0",
    "hapi-cors": "^1.0.1",
    "hapi-webpack-plugin": "^1.3.0",
    "inert": "^4.0.1",
    "lodash": "^4.16.2",
    "moment": "^2.14.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-headroom": "^2.1.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "^2.1.0",
    "tcomb": "^3.2.10"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-root-import": "^4.0.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-babel-root-import": "https://github.com/cle1994/eslint-import-resolver-babel-root-import.git",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.30.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.4.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.1",
    "react-blur": "^0.5.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-runtime",
      [
        "babel-root-import",
        [
          {
            "rootPathPrefix": "~",
            "rootPathSuffix": "src"
          },
          {
            "rootPathPrefix": "_",
            "rootPathSuffix": "src/shared"
          },
          {
            "rootPathPrefix": "$",
            "rootPathSuffix": "src/client"
          },
          {
            "rootPathPrefix": "@",
            "rootPathSuffix": "src/server"
          },
          {
            "rootPathPrefix": "+",
            "rootPathSuffix": "utils"
          },
          {
            "rootPathPrefix": "&",
            "rootPathSuffix": "webpack"
          }
        ]
      ]
    ],
    "env": {
      "test": {
        "sourceMaps": "inline"
      }
    }
  }
}
