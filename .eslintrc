{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "semi": [ 2, "always" ],
    "comma-dangle": 0,
    "no-console": 0,
    "func-names": 0,
    "no-else-return": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "import/imports-first": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0
  },
  "plugins": [
    "babel"
  ],
  "settings": {
    "import/resolver": {
      "babel-root-import": {}
    }
  }
}
